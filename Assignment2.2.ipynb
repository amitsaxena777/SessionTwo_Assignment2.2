     "text": [
      "*\n",
      "**\n",
      "***\n",
      "****\n",
      "*****\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n",
      "\n"
     ]
    }
   ],
   "source": [
    "for i in range(0,5):\n",
    "    for j in range(0,i+1):\n",
    "        print(\"*\",end='')\n",
    "    print()\n",
    "for k in range(6,11):\n",
    "    for j in reversed(range(1,11-k)):\n",
    "        print(\"*\",end='')\n",
    "    print()"
   ]
